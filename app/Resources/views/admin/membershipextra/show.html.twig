{% extends 'adminbase.html.twig' %}

{% block breadcrumbs %}
    <nav class="breadcrumb">
        <a class="breadcrumb-item" href="/">Home</a>
        <a class="breadcrumb-item" href="{{ path('admin_index')}}">Admin</a>
        <a class="breadcrumb-item" href="{{ path('admin_membershipextra_index')}}">Membership Extras</a>
        <span class="breadcrumb-item active">{{ membershipExtra.name }}</span>
    </nav>
{% endblock %}

{% block body %}
    <h1>Membership Extra</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ membershipExtra.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ membershipExtra.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ membershipExtra.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_membershipextra_edit', { 'id': membershipExtra.id }) }}" class="btn btn-primary">Edit</a>

    {{ form_start(delete_form) }}
        <input type="submit" value="Delete" class="btn btn-danger">
    {{ form_end(delete_form) }}
{% endblock %}
