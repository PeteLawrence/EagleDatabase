{% extends 'adminbase.html.twig' %}

{% block body %}
    <h1>Paymenttype</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ paymentType.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ paymentType.type }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_paymenttype_index') }}">Back to the list</a>
