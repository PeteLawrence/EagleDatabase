{% extends 'base.html.twig' %}

{% block body %}
    <h1>MembershipPeriod</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ membershipPeriod.id }}</td>
            </tr>
            <tr>
                <th>Fromdate</th>
                <td>{% if membershipPeriod.fromDate %}{{ membershipPeriod.fromDate|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Todate</th>
                <td>{% if membershipPeriod.toDate %}{{ membershipPeriod.toDate|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_membershipperiod_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_membershipperiod_edit', { 'id': membershipPeriod.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
